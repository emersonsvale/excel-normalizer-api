{
  "version": 2,
  "builds": [
    {
      "src": "app/vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/vercel_app.py"
    }
  ],
  "functions": {
    "app/vercel_app.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
} 